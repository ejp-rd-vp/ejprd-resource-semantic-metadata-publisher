@prefix rr:     <http://www.w3.org/ns/r2rml#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .
@prefix schema: <http://schema.org/> .
@prefix rdf:    <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:   <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ejp:    <http://purl.org/ejp-rd/vocabulary/> .
@prefix owl:    <http://www.w3.org/2002/07/owl#> .
@prefix dct:    <http://purl.org/dc/terms/> .
@prefix dc:     <http://purl.org/dc/elements/1.1/> .
@prefix dcat:   <http://www.w3.org/ns/dcat#> .
@prefix vcard:  <http://www.w3.org/ns/dcat#> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix rml:    <http://semweb.mmlab.be/ns/rml#> .
@prefix ql:     <http://semweb.mmlab.be/ns/ql#> .
@prefix locn:   <http://www.w3.org/ns/locn#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#>.
@base           <http://purl.org/ejp-rd/vocabulary/> .

<#VenueMapping>
    a                     rr:TriplesMap ;
    rml:logicalSource     [ rml:source    "src/main/resources/EJPRDMetadata.json" ;
                            rml:referenceFormulation ql:JSONPath ;
                            rml:iterator             "$" ] ;
    rr:subjectMap         [ rr:template "http://www.w3.org/ns/dcat#{catalogedResources.catalog.catalog}" ;


                            rr:class    dcat:catalog ;

];

    rr:predicateObjectMap [ rr:predicate dct:hasPart ;
                            rr:objectMap [ rml:reference "catalogedResources.resources.title" ; ]
                          ] ;

    rr:predicateObjectMap [ rr:predicate          dct:publisher ;
                            rr:objectMap          [ rml:reference "catalogedResources.organisation.name" ; ];
                          ];

    rr:predicateObjectMap [ rr:predicate dct:distribution ;
                                                    rr:objectMap [ rml:reference "catalogedResources.organisation.name" ]  ];

    rr:predicateObjectMap [ rr:predicate dct:creator ;
                                                    rr:objectMap [ rml:reference "catalogedResources.agent" ; ] ];

    rr:predicateObjectMap [ rr:predicate skos:inScheme ;
                            rr:objectMap [ rml:reference "catalogedResources.agent" ; ] ];

    rr:predicateObjectMap [ rr:predicate dcat:themeTaxonomy ;
                            rr:objectMap [ rml:reference "catalogedResources.agent" ; ] ];

    rr:predicateObjectMap [ rr:predicate dcat:dataset ;
                            rr:objectMap [ rml:reference "catalogedResources.agent" ; ] ];

    rr:predicateObjectMap [ rr:predicate dcat:catalog ;
                            rr:objectMap [ rml:reference "catalogedResources.agent" ; ] ];

    rr:predicateObjectMap [ rr:predicate dcat:spatial ;
                            rr:objectMap [ rml:reference "catalogedResources.agent" ; ] ].
