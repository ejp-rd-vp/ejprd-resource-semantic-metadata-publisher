
@prefix ejp: <http://purl.org/ejp-rd/vocabulary/>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs:<http://www.w3.org/2000/01/rdf-schema#>.
@prefix owl: <http://www.w3.org/2002/07/owl#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix dct: <http://purl.org/dc/terms/>.
@prefix dc: <http://purl.org/dc/elements/1.1/>.
@prefix dcat: <http://www.w3.org/ns/dcat#>.
@prefix vcard: <http://www.w3.org/ns/dcat#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix umbel: <http://umbel.org/umbel#>.
@prefix ql: <http://semweb.mmlab.be/ns/ql#>.
@base  <http://purl.org/ejp-rd/vocabulary/>.



@prefix rr: <http://www.w3.org/ns/r2rml#>.
@prefix rml: <http://semweb.mmlab.be/ns/rml#>.
@prefix xsd: <http://www.w3.org/2001/XMLSchema#>.
@prefix schema: <http://schema.org/>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#lat>.
@prefix gn: <http://www.geonames.org/ontology#>.
@base <http://example.com/ns#>.



<#EJPRDMetadataModel> a ejp:TriplesMap;
  ejp:logicalSource [
  ejp:source "EJPRDMetadataModel.json";
  ejp:referenceFormulation ql:JSONPath;
  ejp:iterator "$";
  ];

  rr:subjectMap [
    rr:subject   "http://www.geonames.org/ontology#";
    rr:template "http://loc.example.com/city/{location.city}";
    rr:class schema:City
  ];

  rr:predicateObjectMap [
    rr:predicate wgs84_pos:lat;
    rr:objectMap [
      rml:reference "venue.latitude"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate wgs84_pos:long;
    rr:objectMap [
      rml:reference "venue.longitude"
    ]
  ];

  rr:predicateObjectMap [
    rr:predicate gn:countryCode;
    rr:objectMap [
      rml:reference "location.country"
    ]
  ].










  "@context": {
    "@base": "http://purl.org/ejp-rd/vocabulary/",
    "ejp": "http://purl.org/ejp-rd/vocabulary/",
    "rdf": "http://www.w3.org/1999/02/22-rdf-syntax-ns#",
    "rdfs": "http://www.w3.org/2000/01/rdf-schema#",
    "owl": "http://www.w3.org/2002/07/owl#",
    "xsd": "http://www.w3.org/2001/XMLSchema#",
    "dct": "http://purl.org/dc/terms/",
    "dc": "http://purl.org/dc/elements/1.1/",
    "dcat": "http://www.w3.org/ns/dcat#",
    "vcard": "http://www.w3.org/ns/dcat#",
    "foaf" : "http://xmlns.com/foaf/0.1/",
    "umbel" : "http://umbel.org/umbel#",
    "id": "dc:identifier",
    "name": "dct:title",
    "city": "vcard:locality",
    "country": "vcard:country-name",
    "description": "dct:description",
    "region" : "vcard:region",
    "datasets": {
      "@id": "dcat:dataset",
      "@type": "@id"
    },
    "theme": {
      "@id": "dcat:theme",
      "@type": "@id"
    },
    "disease_code": {
      "@id": "umbel:relatesToDisease",
      "@type": "@id"
    },
    "themeTaxonomy": {
      "@id": "dcat:themeTaxonomy",
      "@type": "@id"
    },
    "location": "dct:spatial",
    "publisher": "dct:publisher",
    "type": {
      "@id": "rdf:type",
      "@type": "@id"
        }
        }
        }