@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix : <http://example.org/rules/> .
@prefix schema: <http://schema.org/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix afrl:   <http://purl.allotrope.org/ontologies/rol#> .
@prefix sio:    <http://semanticscience.org/resource/> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix iao:    <http://purl.obolibrary.org/obo/> .
@prefix dcat:   <http://www.w3.org/ns/dcat#> .
:TriplesMap a rr:TriplesMap;
            rml:logicalSource [
                  rml:source "A";
                  rml:referenceFormulation ql:JSONPath;
                  rml:iterator "$.catalog[*]"
              ].

:TriplesMap rr:subjectMap [
                              rr:template "http://example.org/catalog/{id}"
                          ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate rdf:type;
                                      rr:objectMap [
                                                       rr:constant dcat:Catalog
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate schema:givenName;
                                      rr:objectMap [
                                                       rml:reference "type"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate schema:dataset;
                                      rr:objectMap [
                                                       rml:reference "dataset"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate dbo:hairColor;
                                      rr:objectMap [
                                                       rml:reference "resources"
                                                   ]
                                  ].
:TriplesMap rr:predicateObjectMap [
                                      rr:predicate dbo:hasPart;
                                      rr:objectMap [
                                                       rml:reference "hasPart"
                                                   ]
                                  ].
:TriplesMap rr:predicateObjectMap [
                                      rr:predicate dbo:homepage;
                                      rr:objectMap [
                                                       rml:reference "homepage"
                                                   ]
                                  ].
:TriplesMap rr:predicateObjectMap [
                                      rr:predicate dbo:record;
                                      rr:objectMap [
                                                       rml:reference "record"
                                                   ]
                                  ].



:TriplesMap rr:predicateObjectMap [
                                      rr:predicate foaf:type;

                                      rr:objectMap [

                                                       rml:reference "dataset.type.enum"
                                                   ]
                                  ].



:TriplesMap rr:predicateObjectMap [
                                      rr:predicate iao:streetAddress;
                                      rr:objectMap [
                                                       rml:reference "dataset.spatial.location.code"

                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:city;
                                      rr:objectMap [
                                                       rml:reference "dataset.spatial.location.city"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate afrl:country;
                                      rr:objectMap [
                                                       rml:reference "dataset.spatial.location.country"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:continent;
                                      rr:objectMap [
                                                       rml:reference "dataset.spatial.location.continent"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:region;
                                      rr:objectMap [
                                                       rml:reference "dataset.spatial.location.region.enum"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:wasGeneratedBy;
                                      rr:objectMap [
                                                       rml:reference "dataset.wasGeneratedBy"
                                                   ]
                                  ].
:TriplesMap rr:predicateObjectMap [
                                      rr:predicate foaf:distribution;

                                      rr:objectMap [
                                                       rml:reference "dataset.distribution";

                                                   ]
                                  ].
