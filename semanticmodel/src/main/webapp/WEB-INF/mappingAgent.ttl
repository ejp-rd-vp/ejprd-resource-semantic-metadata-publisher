@prefix rr:        <http://www.w3.org/ns/r2rml#>.
@prefix rml:       <http://semweb.mmlab.be/ns/rml#>.
@prefix ql:        <http://semweb.mmlab.be/ns/ql#>.
@prefix xsd:       <http://www.w3.org/2001/XMLSchema#>.
@prefix schema:    <http://schema.org/>.
@prefix wgs84_pos: <http://www.w3.org/2003/01/geo/wgs84_pos#lat>.
@prefix gn:        <http://www.geonames.org/ontology#>.
@prefix dcat:      <http://www.w3.org/ns/dcat#> .
@prefix ejp:       <http://purl.org/ejp-rd/vocabulary/> .
@prefix obo:       <http://purl.obolibrary.org/obo/> .
@base <http://example.com/ns#>.

<#CatalogedResourceMapping>
    a                     rr:TriplesMap ;
    rml:logicalSource     [ rml:source               "A" ;
                            rml:referenceFormulation ql:JSONPath ;
                            rml:iterator             "$" ] ;
    rr:subjectMap         [ rr:template "http://ex.com/{agent.id}" ;
                            rr:class    dcat:Agent ] ;

    rr:predicateObjectMap [ rr:predicate ex:person ;
                            rr:objectMap [ rr:parentTriplesMap <#PersonMapping> ] ] ;

    rr:predicateObjectMap [ rr:predicate ex:organisation ;
                            rr:objectMap [ rr:parentTriplesMap <#OrganisationMapping> ] ] .

<#PersonMapping>
    a                     rr:TriplesMap ;
    rml:logicalSource     [ rml:source               "A" ;
                            rml:referenceFormulation ql:JSONPath ;
                            rml:iterator             "$" ] ;
    rr:subjectMap         [ rr:template "http://ex.com/{agent.person.id}" ;
                            rr:class    dcat:Person ] ;
    rr:predicateObjectMap [ rr:predicate foaf:surname ;
                            rr:objectMap [ rml:reference "agent.person.surname" ] ] ;
    rr:predicateObjectMap [ rr:predicate foaf:firstname ;
                            rr:objectMap [ rml:reference "agent.person.firstname" ] ] ;
    rr:predicateObjectMap [ rr:predicate foaf:workInfoHomepage ;
                            rr:objectMap
                                         [ rml:reference
                                                   "agent.person.workInfoHomepage" ] ] ;

    rr:predicateObjectMap [ rr:predicate foaf:emailAddress ;
                            rr:objectMap [ rml:reference "agent.person.emailAddress" ] ] ;

    rr:predicateObjectMap [ rr:predicate foaf:firstname ;
                            rr:objectMap
                                         [ rml:reference
                                                   "agent.person.contactPhoneNumber" ] ] ;

    rr:predicateObjectMap [ rr:predicate foaf:workInfoHomepage ;
                            rr:objectMap
                                         [ rml:reference
                                                   "agent.person.officeDesignation" ] ] ;

    rr:predicateObjectMap [ rr:predicate foaf:emailAddress ;
                            rr:objectMap [ rml:reference "agent.person.publication" ] ] ;

    rr:predicateObjectMap [ rr:predicate foaf:workInfoHomepage ;
                            rr:objectMap
                                         [ rml:reference
                                                   "agent.person.workInfoHomepage" ] ] ;
    rr:predicateObjectMap [ rr:predicate foaf:emailAddress ;
                            rr:objectMap [ rml:reference "agent.person.emailAddress" ] ] ;
    rr:predicateObjectMap [ rr:predicate foaf:firstname ;
                            rr:objectMap
                                         [ rml:reference
                                                   "agent.person.contactPhoneNumber" ] ] ;
    rr:predicateObjectMap [ rr:predicate foaf:workInfoHomepage ;
                            rr:objectMap
                                         [ rml:reference
                                                   "agent.person.officeDesignation" ] ] ;
    rr:predicateObjectMap [ rr:predicate foaf:emailAddress ;
                            rr:objectMap [ rml:reference "agent.person.publication" ] ] .

<#OrganisationMapping>
    a                     rr:TriplesMap ;
    rml:logicalSource     [ rml:source               "A" ;
                            rml:referenceFormulation ql:JSONPath ;
                            rml:iterator             "$" ] ;
    rr:subjectMap         [ rr:template "http://ex.com/{agent.organisation.id}";
                            rr:class    dcat:Organisation] ;

    rr:predicateObjectMap [ rr:predicate foaf:OrgTitle ;
                            rr:objectMap [ rml:reference "agent.organisation.title" ] ] ;
    rr:predicateObjectMap [ rr:predicate foaf:OrgName ;
                            rr:objectMap [ rml:reference "agent.organisation.name" ] ] ;
    rr:predicateObjectMap [ rr:predicate foaf:OrgInfoHomepage ;
                            rr:objectMap
                                         [ rml:reference
                                                   "agent.organisation.description" ] ] ;
    rr:predicateObjectMap [ rr:predicate foaf:OrgAddress ;
                            rr:objectMap [ rml:reference "agent.organisation.homepage" ] ] ;

    rr:predicateObjectMap [ rr:predicate dcat:spatial ;
                            rr:objectMap [ rr:parentTriplesMap <#LocationMapping> ] ] .

<#LocationMapping>
    a                     rr:TriplesMap ;
    rml:logicalSource     [ rml:source               "A" ;
                            rml:referenceFormulation ql:JSONPath ;
                            rml:iterator             "$" ] ;
    rr:subjectMap         [ rr:template "http://ex.com/{agent.organisation.spatial.location.id}";
                                rr:class    ejp:Location ] ;
    rr:predicateObjectMap [ rr:predicate obo:streetAddress ;
                            rr:objectMap [ rml:reference "agent.organisation.spatial.location.streetAddress" ] ] ;
    rr:predicateObjectMap [ rr:predicate obo:city ;
                            rr:objectMap [ rml:reference "agent.organisation.spatial.location.city" ] ] ;
    rr:predicateObjectMap [ rr:predicate sio:country ;
                            rr:objectMap [ rml:reference "agent.organisation.spatial.location.country" ] ] ;
    rr:predicateObjectMap [ rr:predicate dcat:continent ;
                            rr:objectMap
                                         [ rml:reference
                                                   "agent.organisation.spatial.location.continent" ] ] ;

    rr:predicateObjectMap [ rr:predicate foaf:OrgAddress ;
                            rr:objectMap [ rml:reference "agent.organisation.spatial.location.region.enum" ] ] .

