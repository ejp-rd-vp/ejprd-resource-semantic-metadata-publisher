:TriplesMap a rr:TriplesMap;
            rml:logicalSource [
                  rml:source "A";
                  rml:referenceFormulation ql:JSONPath;
                  rml:iterator "$.agent[*]"
              ].

:TriplesMap rr:subjectMap [
                              rr:template "http://example.org/agent/{id}";
                              rr:class foaf:Agent
                          ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate rdf:type;
                                      rr:objectMap [
                                                       rr:constant foaf:Person
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate foaf:family_name;

                                      rr:objectMap [

                                                       rml:reference "person.lastname"

                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate foaf:firstName;
                                      rr:objectMap [
                                                       rml:reference "person.firstname"

                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate iao:IAO_0000429;
                                      rr:objectMap [
                                                       rml:reference "person.emailAddress"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:SIO_001324;
                                      rr:objectMap [
                                                       rml:reference "person.contactPhoneNumber"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
