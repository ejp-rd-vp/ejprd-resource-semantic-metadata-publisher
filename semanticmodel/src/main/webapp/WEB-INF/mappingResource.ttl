@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix : <http://example.org/rules/> .
@prefix schema: <http://schema.org/> .
@prefix afrl:   <http://purl.allotrope.org/ontologies/rol#> .
@prefix sio:    <http://semanticscience.org/resource/> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix iao:    <http://purl.obolibrary.org/obo/> .
@prefix ejp:    <http://purl.org/ejp-rd/vocabulary/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix dcat:   <http://www.w3.org/ns/dcat#> .

:TriplesMap a rr:TriplesMap;
            rml:logicalSource [
                  rml:source "A";
                  rml:referenceFormulation ql:JSONPath;
                  rml:iterator "$.resource[*]"
              ].

:TriplesMap rr:subjectMap [
                              rr:template "http://example.org/resource/{id}";
                              rr:class dcat:Resource
                          ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate rdf:type;
                                      rr:objectMap [
                                                       rr:constant dcat:Resource
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate foaf:family_name;

                                      rr:objectMap [

                                                       rml:reference "lastname"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate foaf:firstName;
                                      rr:objectMap [
                                                       rml:reference "firstname";

                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate iao:IAO_0000429;
                                      rr:objectMap [
                                                       rml:reference "emailAddress"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:SIO_001324;
                                      rr:objectMap [
                                                       rml:reference "contactPhoneNumber"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate afrl:AFRL_0000403;
                                      rr:objectMap [
                                                       rml:reference "officeDesignation"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:SIO_000087;
                                      rr:objectMap [
                                                       rml:reference "publication"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:SIO_000087;
                                      rr:objectMap [
                                                       rml:reference "workInfoHomepage"
                                                   ]
                                  ].
:TriplesMap rr:predicateObjectMap [
                                      rr:predicate foaf:family_name;

                                      rr:objectMap [
                                                       rml:reference "surname";

                                                   ]
                                  ].
:TriplesMap rr:predicateObjectMap [
                                      rr:predicate foaf:firstName;
                                      rr:objectMap [

                                                       rml:reference "first name"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:ContactPoint;
                                      rr:objectMap [
                                                       rml:reference "contactPoint"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:Keyword;
                                      rr:objectMap [
                                                       rml:reference "keyword"
                                                   ]
                                  ].
:TriplesMap rr:predicateObjectMap [
