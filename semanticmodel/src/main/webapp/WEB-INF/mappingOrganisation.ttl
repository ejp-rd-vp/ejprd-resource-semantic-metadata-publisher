@prefix rml: <http://semweb.mmlab.be/ns/rml#> .
@prefix rr: <http://www.w3.org/ns/r2rml#> .
@prefix ql: <http://semweb.mmlab.be/ns/ql#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix : <http://example.org/rules/> .
@prefix schema: <http://schema.org/> .
@prefix afrl:   <http://purl.allotrope.org/ontologies/rol#> .
@prefix sio:    <http://semanticscience.org/resource/> .
@prefix foaf:   <http://xmlns.com/foaf/0.1/> .
@prefix iao:    <http://purl.obolibrary.org/obo/> .
@prefix ejp:    <http://purl.org/ejp-rd/vocabulary/> .
@prefix obo: <http://purl.obolibrary.org/obo/> .
@prefix dcat:   <http://www.w3.org/ns/dcat#> .

:TriplesMap a rr:TriplesMap;
                rml:logicalSource [
                  rml:source "A";
                  rml:referenceFormulation ql:JSONPath;
                  rml:iterator "$.organisation[*]"
              ].

:TriplesMap rr:subjectMap [
                              rr:template "http://example.org/organisation/{id}";
                                  rr:class foaf:Organisation
                          ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate rdf:type;
                                      rr:objectMap [
                                                       rr:constant foaf:Organisation
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate foaf:title;

                                      rr:objectMap [

                                                       rml:reference "title"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate foaf:Name;
                                      rr:objectMap [
                                                       rml:reference "name";

                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate iao:description;
                                      rr:objectMap [
                                                       rml:reference "description"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:homepage;
                                      rr:objectMap [
                                                       rml:reference "homepage"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:SIO_000087;
                                      rr:objectMap [
                                                       rml:reference "publication"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate sio:country;
                                      rr:objectMap [
                                                       rml:reference "country"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                                      rr:predicate dcat:continent;
                                      rr:objectMap [
                                                       rml:reference "location.continent"
                                                   ]
                                  ].

:TriplesMap rr:predicateObjectMap [
                              rr:predicate dcat:city;
                              rr:objectMap [
                                               rml:reference "location.city"
                                           ]
                          ].

:TriplesMap rr:predicateObjectMap [
                              rr:predicate dcat:country;
                              rr:objectMap [
                                               rml:reference "location.country"
                                           ]
                          ].

:TriplesMap rr:predicateObjectMap [
                              rr:predicate dcat:continent;
                              rr:objectMap [
                                               rml:reference "location.continent"
                                            ]
                                ].
:TriplesMap rr:predicateObjectMap [
                                      rr:predicate dcat:continent;
                                      rr:objectMap [
                                                       rml:reference "location.region.enum"
                                                   ]
                                  ].