<#DistributionMapping>
    a                     rr:TriplesMap ;
    rml:logicalSource     [ rml:source               "A" ;
                            rml:referenceFormulation ql:JSONPath ;
                            rml:iterator             "$" ] ;
    rr:subjectMap         [ rr:template "http://ex.com/{Distribution.id}" ;
                            rr:class    dcat:Distribution ] ;

