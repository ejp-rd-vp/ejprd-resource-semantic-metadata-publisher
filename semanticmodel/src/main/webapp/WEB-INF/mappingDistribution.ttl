                            rr:objectMap [ rml:reference "Distribution.modificationDate" ] ] ;
    rr:predicateObjectMap [ rr:predicate ex:language ;
                            rr:objectMap [ rml:reference "Distribution.language" ] ] ;
    rr:predicateObjectMap [ rr:predicate ex:identifier ;
                            rr:objectMap [ rml:reference "Distribution.identifier" ] ] ;


    rr:predicateObjectMap [ rr:predicate ex:theme ;
                            rr:objectMap [ rml:reference "Distribution.theme" ] ] ;
    rr:predicateObjectMap [ rr:predicate ex:type ;
                            rr:objectMap [ rml:reference "Distribution.downloadURL" ] ] ;
